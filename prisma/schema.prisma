datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Admin {
  id        String   @id @default(uuid())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Booking {
  id        String   @id @default(uuid())
  name      String
  email     String
  movie     String
  date      DateTime
  seat      String
  createdAt DateTime @default(now())
}
